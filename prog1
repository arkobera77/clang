#include<stdio.h>

int factorial(int n){
    if(n==1 || n==0){
        return 1;
    }
    else{
        return n*factorial(n-1);
    }
    
}

int main()
{
    int t;
    scanf("%d",&t);
    while(t--){
        int n;
        scanf("%d",&n);
        int a=factorial(n);
        printf("%d\n",a);
    }
    
    return 0;
}
